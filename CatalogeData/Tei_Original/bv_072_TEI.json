            "physDesc": {
              "objectDesc": {
                "supportDesc": {
                  "@material": "mixed",
                  "support": {
                    "p": "paper"
                  },
                  "extent": "ii, 16, ii leaves : 225 x 136 (205 x 100) mm bound to 233 x 140 mm",
                  "foliation": "Modern foliation in pencil added, upper outer recto."
                },
                "layoutDesc": {
                  "layout": "Chiefly grids that are structured as follows: right-hand page: 5 long lines over 16 columns divided into sets of 2, 1, 4, 9 columns with columns divided into rows of 1 header row and three sets of 5 rows; left-hand page: 5 long lines over 18 columns divided into sets of 4, 6, 5, 1, 2 columns with columns divided into rows of 1 header row, three sets of 5 rows. All grids border ruled with heavier rules dividing the sets of columns and rows."
                }
              },
              "scriptDesc": {
                "scriptNote": "Written in nastaʻlīq in black and red ink; pointed."
              },
              "decoDesc": {
