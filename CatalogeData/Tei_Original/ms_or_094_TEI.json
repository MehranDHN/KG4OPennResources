            "physDesc": {
              "objectDesc": {
                "supportDesc": {
                  "@material": "mixed",
                  "support": {
                    "p": "non-European laid paper"
                  },
                  "extent": "38 foliated leaves : 215 x 155 (160 x 95) mm, bound to 205 x 155 x 15 mm"
                },
                "layoutDesc": {
                  "layout": "7 long lines."
                }
              },
              "scriptDesc": {
                "scriptNote": "Written in naskh in black ink; pointed; fully vocalized."
              },
              "bindingDesc": {
                "binding": {
                  "p": "Islamic binding. Full brown leather with blind tooling and central mandorla. Fore-edge and envelope flap. Separate pastedowns of marbled paper with stubs extending across the joints and marbled paper flap linings. Endbands with chevron-pattern secondary sewing."
                }
              }
            },
            "history": {
              "origin": {
                "p": "Copy finished 1 Muḥarram A.H. 1086.",
                "origDate": "1675",
                "origPlace": "Turkey or Syria?"
              },
              "provenance": "Gift of David Eugene Smith, 1931-1934."
            }
